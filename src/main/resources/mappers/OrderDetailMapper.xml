<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tang.goods.mapper.OrderDetailMapper">
    <select id="searchOrderDetailByNo" parameterType="String" resultType="com.tang.goods.entity.OrderDetail">
        select *from
        order_detail
        where no =#{no}
    </select>

    <select id="searchOrderDetailByOrderNo" parameterType="String" resultType="com.tang.goods.entity.OrderDetail">
        select *from
        order_detail
        where orderNo =#{orderNo}
    </select>
</mapper>

